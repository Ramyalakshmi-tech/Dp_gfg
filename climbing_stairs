https://leetcode.com/problems/climbing-stairs/

//memoization
class Solution {
    public int climbStairs(int n) {
        int[] dp=new int[n+1];
        Arrays.fill(dp,-1);
      return climbStairsRecur(n,dp);

    }

    public int climbStairsRecur(int n,int[] dp){
        if(n<0) return 0;
        if(n==0) return 1;
        if(dp[n]!=-1) return dp[n];
        int ans=0;
        ans+=climbStairsRecur(n-1,dp);
        ans+=climbStairsRecur(n-2,dp);
        return dp[n]=ans;

    }
}

//tabulation
